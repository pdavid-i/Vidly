@model Vidly.Models.Movie

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2></h2>
<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre</li>
    <li>Release Date: @Model.getReleased() </li>
    <li>Date Added: @Model.getAdded() </li>
    <li>Left in stock: @Model.inStock </li>
</ul>

